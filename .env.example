# ColonyForge 環境変数設定
# このファイルを .env にコピーして値を設定してください

# LLM API Key (Copilot使用時は不要)
OPENAI_API_KEY=
CLAUDE_CODE_API_KEY=
# ColonyForge API Key (認証有効時に必要)
COLONYFORGE_API_KEY=

# ログレベル (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# GitHub API トークン (GitHub Projection 機能使用時に必要)
GITHUB_TOKEN=